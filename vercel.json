{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|blog|servicos|sobre|lgpd|assets|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
